{
  "name": "custom-express-api",
  "description": "Boilerplate Express API",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prestart": "npm run -s build",
    "build": "babel src -s -D -d dist",
    "start": "node dist",
    "dev": "nodemon -w src --exec \"babel-node src \"",
    "lint": "eslint 'src/**/*.{js,jsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx}'",
    "test": "yarn lint && jest"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "ejs": "~2.5.7",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "mongoose": "^5.4.3",
    "morgan": "~1.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^5.12.1",
    "eslint-plugin-jest": "^22.1.3",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9"
  }
}
